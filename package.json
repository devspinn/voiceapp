{
  "name": "voiceapp",
  "private": true,
  "scripts": {
    "dev": "turbo dev -- --host",
    "build": "turbo build",
    "db:push": "pnpm --filter @voiceapp/db db:push",
    "db:generate": "pnpm --filter @voiceapp/db generate",
    "db:migrate": "pnpm --filter @voiceapp/db migrate"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "devDependencies": {
    "dotenv-cli": "^11.0.0",
    "turbo": "^2.4.0",
    "typescript": "^5.7.0"
  },
  "packageManager": "pnpm@10.19.0"
}
